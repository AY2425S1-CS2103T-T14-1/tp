@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlyPrudy" as ReadOnlyPrudy
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class Prudy
Class ModelManager
Class UserPrefs

Class UniqueClientList
Class Client
Class Address
Class Email
Class Name
Class Phone
Class Tag
Class PolicySet
Class LifePolicy
Class HealthPolicy
Class EducationPolicy
Class "{abstract}\nPolicy" as Policy
Class ClaimList
Class Claim
Class PremiumAmount
Class CoverageAmount
Class ExpiryDate
Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

Prudy .up.|> ReadOnlyPrudy

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyPrudy
ModelManager -left-> "1" Prudy
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

Prudy *--> "1" UniqueClientList
UniqueClientList --> "~* all" Client
Client *--> Name
Client *--> Phone
Client *--> Email
Client *--> Address
Client *--> "*" Tag
Client *--> PolicySet
PolicySet o--> LifePolicy
PolicySet o--> HealthPolicy
PolicySet o--> EducationPolicy
LifePolicy --|> Policy
HealthPolicy --|> Policy
EducationPolicy --|> Policy
Policy *--> ClaimList
Policy *--> PremiumAmount
Policy *--> CoverageAmount
Policy *--> ExpiryDate
ClaimList o--> "*" Claim

Client -[hidden]up--> I
UniqueClientList -[hidden]right-> I

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

ModelManager --> "~* filtered" Client
@enduml
